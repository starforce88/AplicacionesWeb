<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>ingreso</servlet-name>
        <servlet-class>servlets.Ingreso</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registra</servlet-name>
        <servlet-class>servlets.Registra</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Edita</servlet-name>
        <servlet-class>servlets.Edita</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ingreso</servlet-name>
        <url-pattern>/ingreso</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registra</servlet-name>
        <url-pattern>/Registra</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Edita</servlet-name>
        <url-pattern>/Edita</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
